{
  "name": "drdplus/destruction-calculator",
  "license": "MIT",
  "type": "project",
  "description": "Have you broke in or broke a sword?",
  "keywords": [
    "DrD",
    "DrD plus",
    "destruction",
    "destroy",
    "crush",
    "calculator"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "/var/www/kalkulator.skeleton/"
    },
    {
      "type": "vcs",
      "url": "/var/www/utok.skeleton/"
    },
    {
      "type": "vcs",
      "url": "/var/www/rules.skeleton/"
    }
  ],
  "require": {
    "php": ">=7.1",
    "drdplus/tables": "~2.0",
    "drdplus/skills": "~2.0",
    "drdplus/destruction": "~2.0",
    "drdplus/attack-skeleton": "~2.0",
    "drdplus/codes": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0",
    "roave/security-advisories": "dev-master"
  },
  "scripts": {
    "pre-autoload-dump": [
      "rm -f ./DrdPlus/Tests/CalculatorSkeleton && ln -s ../../vendor/drdplus/calculator-skeleton/DrdPlus/Tests/CalculatorSkeleton ./DrdPlus/Tests"
    ]
  }
}
